    <Method Name="WhenMonthToStringReturnsCorrectMonthName" Id="{4be9c321-14b4-4109-98ef-f805349c3ea1}">
      <Declaration><![CDATA[METHOD PRIVATE WhenMonthToStringReturnsCorrectMonthName
VAR
    ActualMonth : STRING(10);
    ExpectedMonth : STRING(10) := 'January';
    MTH : INT := 1;
    LANG : INT := 1; // Adjust as needed for your language table
    LX : INT := 0;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST(__POUNAME());

// @TEST-RUN
ActualMonth := MONTH_TO_STRING(MTH:=MTH, LANG:=LANG, LX:=LX);

// @TEST-ASSERT
AssertEquals_STRING(ExpectedMonth, ActualMonth, 'MONTH_TO_STRING did not return expected month name');

TEST_FINISHED();]]></ST>
      </Implementation>
    </Method>
    <Method Name="WhenReplaceAllReturnsExpectedResult" Id="{9e8c7cae-3db6-4777-ad08-535bc2749d0c}">
      <Declaration><![CDATA[METHOD PRIVATE WhenReplaceAllReturnsExpectedResult
VAR
    ActualStr : STRING(30);
    ExpectedStr : STRING(30) := 'foo bar foo bar';
    TestStr : STRING(30) := 'foo baz foo baz';
    Src : STRING(10) := 'baz';
    Rep : STRING(10) := 'bar';
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST(__POUNAME());

// @TEST-RUN
ActualStr := REPLACE_ALL(TestStr, Src, Rep);

// @TEST-ASSERT
AssertEquals_STRING(ExpectedStr, ActualStr, 'REPLACE_ALL did not return expected result');

TEST_FINISHED();]]></ST>
      </Implementation>
    </Method>
    <Method Name="WhenSubtringExistsExpectPosition" Id="{f43cd479-0c2b-4ed5-ab04-cc6edd49aa4c}">
      <Declaration><![CDATA[METHOD PRIVATE WhenSubtringExistsExpectPosition
VAR
	ActualPosition : INT;
	ExpectedPosition : INT := 11;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST(__POUNAME());

// @TEST-RUN
ActualPosition := FINDP(str:='Premature optimization is the root of all evil', src:='optimization', pos:=1);

// @TEST-ASSERT
AssertTrue(ActualPosition=ExpectedPosition, 'Substring not found in target string');

TEST_FINISHED();]]></ST>
      </Implementation>
    </Method>
