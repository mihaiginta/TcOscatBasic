]]></ST>
    </Implementation>
    <Method Name="WhenCapitalizeReturnsExpectedResult" Id="{4ecc6878-d05e-4509-b226-6ee77bd819d6}">
      <Declaration><![CDATA[METHOD PRIVATE WhenCapitalizeReturnsExpectedResult
VAR
    ActualStr : STRING(20);
    ExpectedStr : STRING(20) := 'Hello World';
    TestStr : STRING(20) := 'hello world';
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST(__POUNAME());

// @TEST-RUN
ActualStr := CAPITALIZE(TestStr);

// @TEST-ASSERT
AssertEquals_STRING(ExpectedStr, ActualStr, 'CAPITALIZE did not return expected result');

TEST_FINISHED();]]></ST>
      </Implementation>
    </Method>
    <Method Name="WhenCountCharReturnsExpectedResult" Id="{9886ba9b-94c1-45af-afd8-7233098d230d}">
      <Declaration><![CDATA[METHOD PRIVATE WhenCountCharReturnsExpectedResult
VAR
    ActualCount : INT;
    ExpectedCount : INT := 3;
    TestStr : STRING(20) := 'banana';
    TestChar : BYTE := 97; // 'a'
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST(__POUNAME());

// @TEST-RUN
